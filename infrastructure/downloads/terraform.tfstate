          "attributes": {
            "arn": "arn:aws:cloudfront::720237151757:function/rinawarp-download-trigger-prod",
            "code": "function handler(event) {\n  var request = event.request;\n  var uri = request.uri;\n      \n  // Only trigger for actual downloads\n  if (uri.endsWith('.dmg') || uri.endsWith('.exe') || uri.endsWith('.AppImage')) {\n    // Invoke Lambda asynchronously\n    var lambda = new aws.lambda();\n    lambda.invoke({\n      FunctionName: 'rinawarp-process-downloads-prod',\n      InvocationType: 'Event',\n      Payload: JSON.stringify({\n        cf: {\n          request: request\n        }\n      })\n    }).promise();\n  }\n      \n  return request;\n}\n",
